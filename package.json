{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-webserver": "http-server -p 8080 /app",
    "stop-webserver": "if pgrep http-server; then pkill http-server; fi",
    "restart-webserver": "npm run stop-webserver && npm run start-webserver",
    "dev": "npm run stop-webserver && node_modules/.bin/vite",
    "build": "npm run restart-webserver && node_modules/.bin/vite build",
    "preview": "npm run stop-webserver && node_modules/.bin/vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0"
  }
}
